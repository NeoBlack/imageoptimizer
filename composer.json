{
  "name": "christophlehmann/imageoptimizer",
  "type": "typo3-cms-extension",
  "description": "Optimize uploaded/processed images with binaries of your choice",
  "keywords": [
    "TYPO3",
    "extension",
    "optipng",
    "jpegoptim",
    "lossless",
    "progressive"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.0,<8.9.99"
  },
  "autoload": {
    "classmap": [
      "Classes/"
    ]
  },
  "replace": {
    "imageoptimizer": "self.version"
  }
}
